{
  "name": "dayofr/sfdc-rest-client",
  "description": "A SalesForce REST API PHP Wrapper",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Dayo Fr",
      "email": "dayo@dayo.fr"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "php": "7.4.*",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Dayofr\\": "src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "phpstan/phpstan": "^0.12.25",
    "vlucas/phpdotenv": "^4.1"
  },
  "autoload-dev": {
    "psr-4": {
      "Dayofr\\Tests\\": "tests/"
    }
  }
}
